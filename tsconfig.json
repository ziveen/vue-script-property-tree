{
	"compilerOptions": {
	  "target": "es2022",
	  "lib": ["es2022"],
	  "module": "ESNext",
	  "moduleResolution": "Bundler",
	  "resolveJsonModule": true,
	  "strict": true,
	  "noUncheckedIndexedAccess": true,
	  "noEmit": true,
	  "esModuleInterop": true,
	  "isolatedModules": true,
	  "skipLibCheck": true
	}
  }